						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float3 temp_cast_0 = (1.0).xxx;
				float2 vertexToFrag10_g1 = IN.ase_texcoord3.xy;
				float2 UV11_g1 = vertexToFrag10_g1;
				float4 localSampleLightmapHD11_g1 = SampleLightmapHD11_g1( UV11_g1 );
				#ifdef UNITY_LIGHTMAP_RGBM_ENCODING
				float4 staticSwitch15_g1 = float4(34.49324,2.2,0,0);
				#else
				float4 staticSwitch15_g1 = float4(2,2.2,0,0);
				#endif
				#ifdef UNITY_LIGHTMAP_FULL_HDR
				float4 staticSwitch13_g1 = float4(0,0,0,0);
				#else
				float4 staticSwitch13_g1 = staticSwitch15_g1;
				#endif
				float3 decodeLightMap6_g1 = DecodeLightmap(localSampleLightmapHD11_g1,staticSwitch13_g1);
				#ifdef _LIGHTMAP_ON
				float3 staticSwitch18 = decodeLightMap6_g1;
				#else
				float3 staticSwitch18 = temp_cast_0;
				#endif
				float2 uv_BaseMap = IN.ase_texcoord3.zw * _BaseMap_ST.xy + _BaseMap_ST.zw;
				float4 tex2DNode5 = SAMPLE_TEXTURE2D( _BaseMap, sampler_BaseMap, uv_BaseMap );
				
				float Alpha13 = tex2DNode5.a;
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( float4( staticSwitch18 , 0.0 ) * ( ( tex2DNode5 * _MainColor * _Intensity ) * IN.ase_color ) ).rgb;
				float Alpha = Alpha13;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
					LODDitheringTransition( IN.clipPos.xyz, unity_LODFade.x );
				#endif

				#ifdef ASE_FOG
					Color = MixFog( Color, IN.fogFactor );
				#endif

				return half4( Color, Alpha );
			}

			ENDHLSL
		}

		
