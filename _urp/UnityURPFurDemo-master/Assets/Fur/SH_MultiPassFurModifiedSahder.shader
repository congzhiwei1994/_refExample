            #pragma shader_feature _ _GI_ON
			#pragma multi_compile _ LIGHTMAP_ON
			#pragma multi_compile _ _MAIN_LIGHT_SHADOWS
			#pragma shader_feature _NORMALMAP
			#pragma shader_feature _ _RECEIVE_SHADOWS
